# PostgreSQL Database URL for Prisma
# Schema should be 'leave_tracker' to isolate from other v6 services
DATABASE_URL="postgresql://user:password@localhost:5432/leave_tracker?schema=public"

# JWT Authentication (from tc-core-library-js)
AUTH_SECRET="mysecret"
VALID_ISSUERS='["https://topcoder-dev.auth0.com/","https://api.topcoder.com"]'

# Optional: Prisma transaction timeout (milliseconds)
LEAVE_SERVICE_PRISMA_TIMEOUT=10000
